  "bugs": {
    "url": "https://github.com/otbe/di/issues"
  },
  "homepage": "https://github.com/otbe/di#readme",
  "dependencies": {},
  "devDependencies": {
    "@mercateo/ws": "^2.1.0",
    "@types/expect": "^1.20.1",
    "@types/mocha": "^2.2.41",
    "babel-polyfill": "^6.23.0",
    "expect": "^1.20.2",
    "mocha": "^3.5.0",
    "reflect-metadata": "^0.1.10"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.X"
  }
