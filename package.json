{
  "name": "simple-ts-di",
  "version": "3.1.0",
  "description": "Simple DI written for TypeScript, no deps",
  "main": "dist-release/index.js",
  "typings": "dist-release/src/index.d.ts",
  "ws": {
    "type": "browser"
  },
  "scripts": {
    "build": "ws b -p",
    "test:ci": "ws lint && jest --config tests/jest.config.js",
    "test": "jest --config tests/jest.config.js",
    "test:watch": "yarn test --watch",
    "preversion": "yarn test:ci",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "TypeScript",
    "dependency",
    "injection",
    "DI"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otbe/di.git"
  },
  "author": "Benjamin Otto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otbe/di/issues"
  },
  "homepage": "https://github.com/otbe/di#readme",
  "dependencies": {},
  "devDependencies": {
    "@mercateo/ws": "^2.11.0",
    "@types/jest": "^23.1.3",
    "jest": "^23.2.0",
    "reflect-metadata": "^0.1.10"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.X"
  }
}
