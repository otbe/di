  "devDependencies": {
    "@mercateo/ws": "^1.0.2",
    "@types/expect": "^1.20.1",
    "@types/mocha": "^2.2.41",
    "expect": "^1.20.2",
    "mocha": "^3.3.0",
    "reflect-metadata": "^0.1.10"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.X"
